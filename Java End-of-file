import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = 0;
        while(sc.hasNext()){                      //判断sc有没有下一行用户输入 condition语句
            n++;
            String s = sc.nextLine();
            System.out.println(n + " " + s);      //建立content
        }

        
    }
}
